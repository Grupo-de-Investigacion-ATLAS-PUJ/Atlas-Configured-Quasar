<?xml version="1.0" encoding="UTF-8"?>
<d:design xmlns:d="http://cern.ch/quasar/Design" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" projectShortName="Wiener" xsi:schemaLocation="http://cern.ch/quasar/Design Design.xsd ">
  <d:class name="CanBus">
    <d:devicelogic/>
    <d:cachevariable name="statisticsTotalTx" addressSpaceWrite="forbidden" initializeWith="valueAndStatus" dataType="OpcUa_UInt32" initialStatus="OpcUa_BadWaitingForInitialData" nullPolicy="nullForbidden" initialValue="0"/>
    <d:cachevariable name="statisticsTxRate" addressSpaceWrite="forbidden" initializeWith="valueAndStatus" dataType="OpcUa_Float" initialStatus="OpcUa_BadWaitingForInitialData" nullPolicy="nullForbidden" initialValue="0"/>
    <d:cachevariable name="statisticsTotalRx" addressSpaceWrite="forbidden" initializeWith="valueAndStatus" dataType="OpcUa_UInt32" initialStatus="OpcUa_BadWaitingForInitialData" initialValue="0" nullPolicy="nullForbidden"/>
    <d:cachevariable name="statisticsRxRate" addressSpaceWrite="forbidden" initializeWith="valueAndStatus" dataType="OpcUa_Float" initialStatus="OpcUa_BadWaitingForInitialData" initialValue="0" nullPolicy="nullForbidden"/>
    <d:cachevariable name="portErrorDescription" addressSpaceWrite="forbidden" initializeWith="valueAndStatus" dataType="UaString" initialStatus="OpcUa_Good" initialValue="" nullPolicy="nullForbidden"/>
  </d:class>
  <d:root>
    <d:hasobjects instantiateUsing="configuration" class="CanBus"/>
  </d:root>
</d:design>
